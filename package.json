		"file-loader": "^6.2.0",
		"glob": "^11.0.0",
		"http-server": "^14.1.1",
		"husky": "^9.1.6",
		"jsdom": "^25.0.1",
		"jsonc-parser": "^3.3.1",
		"mocha": "^10.8.2",
		"monaco-editor-core": "0.52.0",
		"parcel": "^2.12.0",
		"pin-github-action": "^1.9.1",
		"playwright": "^1.48.2",
		"prettier": "^3.3.3",
		"pretty-quick": "^4.0.0",
		"requirejs": "^2.3.7",
		"shelljs": "^0.8.5",
		"style-loader": "^4.0.0",
		"terser": "^5.36.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.3",
		"vite": "^5.4.10",
		"vscode-css-languageservice": "6.3.1",
		"vscode-html-languageservice": "5.3.1",
		"vscode-json-languageservice": "5.4.1",
		"vscode-languageserver-textdocument": "^1.0.12",
		"vscode-languageserver-types": "3.17.5",
		"vscode-uri": "3.0.8",
		"webpack": "^5.96.1",
		"yaserver": "^0.4.0"
